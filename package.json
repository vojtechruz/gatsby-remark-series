{
  "license": "MIT",
  "version": "0.0.1",
  "name": "gatsby-remark-series-toc",
  "description": "Gatsby remark plugin that creates table of contents for series",
  "author": {
    "name": "Patricio Trevino",
    "email": "patricio@weirdpattern.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weirdpattern/gatsby-remark-series-toc"
  },
  "bugs": {
    "url": "https://github.com/weirdpattern/gatsby-remark-series-toc/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "gatsby-plugin",
    "gatsby-remark",
    "remark",
    "series",
    "toc",
    "table of content",
    "plugin"
  ],
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist",
    "clean": "npx rimraf node_modules dist build",
    "commit": "commit",
    "format": "prettier --write '*.{js,jsx,md}'",
    "lint": "eslint . --ext .js,.jsx",
    "test": "jest --config .jestrc.js",
    "test:silent": "yarn run test --reporters jest-dot-reporter",
    "test:verbose": "yarn run test --verbose",
    "test:coverage": "rimraf coverage && yarn run test --coverage --runInBand --no-cache",
    "release": "yarn run build && ./node_modules/.bin/standard-version"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/prompt-cli": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsdoc": "^4.7.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.6.0",
    "jest-dot-reporter": "^1.0.7",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "standard-version": "^5.0.2"
  },
  "dependencies": {
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.kebabcase": "^4.1.1"
  },
  "peerDependencies": {
    "gatsby": "*",
    "gatsby-transformer-remark": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md}": [
      "prettier --write '*.{js,jsx,md}'",
      "git add ."
    ]
  }
}
